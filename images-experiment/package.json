    "jsdom": "^25.0.1",
    "nodemon": "^3.1.7",
    "prettier": "^3.1.0",
    "storybook": "^8.4.4",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10",
